    <!--************************************
			Wrapper Start
	*************************************-->
    <div id="tg-wrapper" class="tg-wrapper tg-haslayout">
      <!--************************************
				Header Start
		*************************************-->
      <%- include ./components/header%>
      <!--************************************
				Header End
        *************************************-->
      <!--************************************
				Main start
        *************************************-->
      <div
        class="tg-innerbanner tg-haslayout tg-parallax tg-bginnerbanner"
        data-z-index="-100"
        data-appear-top-offset="600"
        data-parallax="scroll"
        data-image-src="/static/images/parallax/bgparallax-07.jpg"
      >
        <div class="container">
          <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
              <div class="tg-innerbannercontent">
                <h1>Check out</h1>
                <ol class="tg-breadcrumb">
                  <li><a href="javascript:void(0);">home</a></li>
                  <li class="tg-active">Check out</li>
                </ol>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="tg-sectionspace tg-haslayout"></div>
      <form action="" method="POST">
        <div class="container-fluid">
          <table
            class="table table-hover table-bordered"
            style="font-size:1.3em"
          >
            <thead>
              <tr>
                <th>Tên sản phẩm</th>
                <th>Đơn giá</th>
                <th>Số lượng</th>
                <th>Thành tiền</th>
              </tr>
            </thead>
            <tbody>
              <%if(data.basket){%> <%let tong = 0;%> <% for (let i = 0; i <
              data.basket.length; i++) { %>
              <tr>
                <td><%= data.basket[i].ten_sach%></td>
                <td><%= Math.floor(data.basket[i].gia_ban/1000)%>.000 đ</td>
                <td><%= data.basket[i].so_luong%></td>
                <td>
                  <%= Math.floor(data.basket[i].gia_ban *
                  data.basket[i].so_luong/ 1000)%>.000 đ <%tong +=
                  data.basket[i].gia_ban * data.basket[i].so_luong%>
                </td>
              </tr>

              <% } %>
              <tr>
                <td colspan="3"><strong>Tổng tiền:</strong></td>

                <td><%= tong%> đ</td>
              </tr>
              <% } %>
            </tbody>
          </table>

          <!-- <a type="button" href="" class="btn btn-success pull-right">Thanh toán</a> -->
          <div class="text-center" style="margin-top: 30px">
            <button type="submit" class="btn btn-success btn-lg">
              Thanh toán
            </button>
          </div>
        </div>
      </form>
    </div>
    <style>
      .tien {
        margin-right: 100px;
      }
    </style>

    <div class="tg-sectionspace tg-haslayout"></div>
