														<ul class="tg-bookscategories">
															<li>
																<a href="/products/<%= data.products[i].ma_the_loai %>">
																	<%= data.products[i].ten_the_loai %>
																</a>
															</li>
														</ul>
														<div class="tg-themetagbox"><span class="tg-themetag">sale</span></div>
														<div class="tg-booktitle">
															<h3><a href="/product/<%= data.products[i].ma_sach %>">
																	<%= data.products[i].ten_sach %></a></h3>
														</div>
														<span class="tg-bookwriter">By: <a href="javascript:void(0);">
																<%= data.products[i].tac_gia %></a></span>
